<project type="bio">
  <property name="target.model" value="genomic"/>
  <property name="source.location" location="../bio/sources/"/>
  <property name="common.os.prefix" value="common"/>
  <property name="intermine.properties.file" value="thalemine.properties"/>
  <property name="default.intermine.properties.file" location="../default.intermine.integrate.properties"/>
  <sources>

  <source name="aip-gff" type="aip-gff" dump="true">
    <property name="gff3.taxonId" value="3702"/>
    <property name="gff3.seqDataSourceName" value="JCVI"/>
    <property name="gff3.dataSourceName" value="JCVI"/>
    <property name="gff3.seqClsName" value="Chromosome"/>
    <property name="gff3.dataSetTitle" value="Arabidopsis thaliana gff general annotation"/>
    <property name="src.data.dir" location="/usr/local/projects/AIP/ThaleMineData/TAIR10_refined/gff/" />
  </source>

  <source name="at-agi" type="bar-lookups" dump="true">
    <property name="source.db.name" value="bar-lu"/>
  </source>

  <source name="hormone-expressions" type="bar-expressions" dump="true">
    <property name="source.db.name" value="bar-he"/>
  </source>

  <source name="tissue-expressions" type="bar-expressions" dump="true">
    <property name="source.db.name" value="bar-te"/>
    <property name="dataSourceName" value="atgenexp"/>
    <property name="dataSetTitle" value="BAR Expression Data: tissue"/>
  </source>

  <source name="pathogen-expressions" type="bar-expressions" dump="true">
    <property name="source.db.name" value="bar-pe"/>
    <property name="dataSourceName" value="atgenexp_pathogen"/>
    <property name="dataSetTitle" value="BAR Expression Data: pathogen"/>
  </source>


  </sources>
</project>
